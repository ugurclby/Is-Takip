@model AppUserListViewModel

<div class="border-right" id="sidebar-wrapper">
    <div class="sidebar-heading">
        <img src="~/img/@Model.Picture" alt="@Model.Name" style="height:10rem!important;width:10rem!important;" class="w-100 img-thumbnail" />
    </div>
    <div id="DV_Liste" class="list-group list-group-flush">
        <a id="a_IsEmri" asp-action="Index" asp-controller="IsEmri" class="list-group-item list-group-item-action"><i class="fas fa-arrow-alt-circle-down mr-2"></i> Gelen İş Emirleri</a>
        <a id="a_TamamIsler" asp-action="Index" asp-controller="Gorev" class="list-group-item list-group-item-action"><i class="fas fa-arrow-alt-circle-up mr-2"></i> Tamamladığım İşler</a>
        <a id="a_Profil" asp-action="Index" asp-controller="Profil" class="list-group-item list-group-item-action"><i class="fas fa-user mr-2"></i> Profilim</a>
        <a id="a_Dene3" asp-action="Index" asp-controller="Aciliyet" class="list-group-item list-group-item-action"><i class="fas fa-bell mr-2"></i> Bildirimler</a>
        <a id="a_cikis" asp-area="" asp-action="CikisYap" asp-controller="Home" class="list-group-item list-group-item-action"><i class="fas fa-sign-out-alt mr-2"></i> Çıkış Yap</a>
    </div>
</div>