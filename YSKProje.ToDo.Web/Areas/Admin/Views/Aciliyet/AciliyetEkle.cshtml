@model AciliyetModel
@{
    ViewData["Title"] = "AciliyetEkle";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
 <form class="w-75 mx-auto p-3" method="post">
     <div class="form-group">
         <label asp-for="@Model.Tanim"></label>
         <span asp-validation-for="@Model.Tanim" class="text-danger"></span>
         <input asp-for="@Model.Tanim" type="text" class="form-control"/>
     </div>
     <button type="submit" class="btn btn-info btn-sm">Kaydet</button>
 </form>
 
@section JavaScript {
    <script src="~/lib/jqueryValidate/jquery.validate.min.js"></script>
    <script src="~/lib/jqueryUnob/jquery.validate.unobtrusive.min.js"></script>
}